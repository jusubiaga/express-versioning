{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/code"
    },    
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "theme": "Cerulean"
    }
}